{
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "gunicorn --bind 0.0.0.0:$PORT app:app",

    "restartPolicyType": "ON_FAILURE",
  "environments": {
    "production": {
      "variables": {
        "FLASK_ENV": "production",
        "DEBUG": "False"
      }
    }
  }

}
